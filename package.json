{
  "name": "phaser3-template",
  "productName": "Phaser 3 Template Project",
  "version": "0.1.0",
  "description": "Phaser 3 Template Project",
  "main": "electron/electron-main.js",
  "scripts": {
    "build-electron": "webpack --config webpack/webpack-electron.js",
    "start-electron-dev-part1": "webpack --watch --config webpack/webpack-electron.js",
    "start-electron-dev-part2": "electron .",
    "start-electron-dev": "npm-run-all --parallel start-electron-dev-part1 start-electron-dev-part2",
    "build": "webpack --config webpack/webpack-prod.js",
    "start-dev": "webpack-dev-server --config webpack/webpack-base.js --open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gooeywizard/phaser3-template.git"
  },
  "author": "GooeyWizard <thegooeywizard@gmail.com> (https://gooeywizard.com)",
  "license": "MIT",
  "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
  "bugs": {
    "url": "https://github.com/gooeywizard/phaser3-template/issues"
  },
  "homepage": "https://github.com/gooeywizard/phaser3-template#readme",
  "dependencies": {
    "phaser": "^3.16.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-loader": "^8.0.6",
    "electron": "^5.0.8",
    "file-loader": "^4.1.0",
    "html-webpack-plugin": "^5.5.0",
    "npm-run-all": "^4.1.5",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "webpack": "^4.37.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  }
}
